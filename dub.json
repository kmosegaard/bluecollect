{
    "name": "bluecollect",
    "authors": [ "kmosegaard" ],
    "license": "MIT",
    "description": "A minimal D application.",
    "copyright": "Copyright Â© 2022, kmosegaard",
    "lflags": [
        "-Lusr/lib"
    ],
    "libs": [
        "stdc++",
        "duckdb_static",
        "duckdb_fastpforlib",
        "duckdb_fmt",
        "duckdb_fsst",
        "duckdb_hyperloglog",
        "duckdb_miniz",
        "duckdb_mbedtls",
        "duckdb_pg_query",
        "duckdb_re2",
        "duckdb_utf8proc",
        "parquet_extension",
        "jemalloc_extension"
    ],
    "preBuildCommands": [
        "cmake -S external/duckdb -B build -G Ninja -DCMAKE_BUILD_TYPE=Release -DBUILD_SHELL=OFF -DBUILD_PARQUET_EXTENSION=ON -DBUILD_UNITTESTS=OFF -DCMAKE_INSTALL_PREFIX=usr",
        "cmake --build build -t install"
    ]
}
